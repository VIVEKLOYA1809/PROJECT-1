   "source": [
    "import pandas as pd\n",
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "b4f455a9",
   "metadata": {},
   "outputs": [],
   "source": [
    "skill=pd.read_excel(\"skill_data.xlsx\")\n",
    "x=skill['Skills'].values"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "bed6bce2",
   "metadata": {},
   "outputs": [],
   "source": [
    "web_developer_skills=['django','html','css','nodejs','reactjs','flask','laravel']\n",
    "machine_learning_skills=['nosql','pandas','numpy','matplotlib','seaborn','scipy']\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "90162af1",
   "metadata": {},
   "outputs": [],
   "source": [
    "sk=[]\n",
    "length=[]\n",
    "for i in range(len(skill)):\n",
    "    l = [str[1:-1] for str in x[i][1:-1].split(', ')]\n",
    "    length.append(len(np.unique(l)))\n",
    "    p=np.intersect1d(l,web_developer_skills)\n",
    "    q=np.intersect1d(l,machine_learning_skills)\n",
    "    if len(p)>=3:\n",
    "        sk.append('web_development')\n",
    "    elif len(q)>=3:\n",
    "        sk.append('machine_learning')\n",
    "    else:\n",
    "        sk.append('No_skill')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "1d9f4969",
   "metadata": {},
   "outputs": [],
   "source": [
    "skill['Designation']=sk\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "id": "eb57231d",
   "metadata": {},
   "outputs": [],
   "source": [
    "skill['No_of_Skills']=length"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "f33d23a6",
